package com.commons;
import org.bean.User;


public class UserImg  {
			

	public static void main(String[] args) {
	
	
	UserImg.getPathById(3);
}
	
	public static String getPathById (int id) {
	
		
		
		User appoUser = new User();
		
		if (appoUser.getImgPath() == null) {
			
			appoUser.setImgPath("./default_image.jpg");
			
		}

		System.out.println(appoUser.getImgPath());
		return appoUser.getImgPath();			

		
	}
	
}
		
