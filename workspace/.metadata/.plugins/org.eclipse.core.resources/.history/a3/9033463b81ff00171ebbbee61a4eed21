package esempi;

public class EsempioSwitch {

	public static void main(String[] args) {
		
		switch("primo") {
		case "primo" :
			System.out.println("primo");
		case "secondo" :
			System.out.println("secondo");
			break;
		case "terzo" :
			System.out.println("terzo");
		}
		
		int x = (10 == 11 ? 1 : 2);
		
	}

}
