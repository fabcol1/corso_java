package esempi;

public class StampaParametro {

	public static void main(String[] args) {
		
		System.out.println(args[0] + " " + args[1] + " " + args[2]);
		
		
	}
	
	public void esercizio1(String[] args) {
		for(String s : args) {
			if(s!=null) {
				System.out.print(s + " " );
			}
		}
	}
	
	public void esercizio2(String[] args) {
		for(String s : args) {
			if(s!=null) {
				int x;
				try {
					x = Integer.parseInt(s);
				} catch (Exception e) {
					
				}
				System.out.print( x + " " );
			}
		}
	}

}

// 1 - inserisco 3 stringhe
// 2 - inserisco 3 numeri
// 3 - inserisco 2 numeri + stringa che identifica tipo di operazione + - / * %