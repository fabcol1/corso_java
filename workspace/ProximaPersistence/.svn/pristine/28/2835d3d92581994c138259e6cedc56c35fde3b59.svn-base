package org.proxima.survey.helper;

import org.proxima.survey.entities.Question;
import org.proxima.survey.rest.bean.RequestQuestion;

/**
 * @author maurizio
 *
 */
public class QuestionConverter {

	public static Question questionFromRequestToEntity(RequestQuestion rq) {
		Question q = new Question();
		q.setLabel(rq.getLabel());
		q.setDescription(rq.getDescription());
		q.setAnsa(rq.getAnsa());
		q.setAnsb(rq.getAnsb());
		q.setAnsc(rq.getAnsc());
		q.setAnsd(rq.getAnsd());
		q.setAnse(rq.getAnse());
		q.setAnsf(rq.getAnsf());
		q.setCansa(rq.getCansa());
		q.setCansb(rq.getCansb());
		q.setCansc(rq.getCansc());
		q.setCansd(rq.getCansd());
		q.setCanse(rq.getCanse());
		q.setCansf(rq.getCansf());
		return q;

		// TO DO!!!!!
	}

}
